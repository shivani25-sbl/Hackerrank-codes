#include<stdio.h>
int main(){
    int octalnum;
    int decnum = 0 ,base = 1;
    while(octalnum != 0){
        int rem =octalnum % 10;
        decnum += rem *base;
        base *= 8;
        octalnum/= 10;

    }
     return 0;
}

int main(){
    int decnum, bin=0,place=1;
    while(decnum !=0){
        int rem = decnum % 2;
        bin = bin + rem * place;
        decnum /=2;
        place = place * 10;
    }
    
    return bin;
}
int main(){
 int num ;
 printf("enter octal number: ");
 scanf("%d", &num);
 int binary = converttobinary(convertoctaltodecimal(num));
 printf("binary of octal number %d is %d ",num, binary);
 return 0;

}
