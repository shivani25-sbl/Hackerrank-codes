#include<stdio.h>
int main(){
    int num;
    printf("Enter octal number: ");
    scanf("%d",&num);
    int decnum = 0 ,base = 1;
    while(num != 0){
        int rem =num % 10;
        decnum += rem *base;
        base *= 8;
        num/= 10;

    }
     printf("decimal number = %d" , decnum);
     return 0;
}
