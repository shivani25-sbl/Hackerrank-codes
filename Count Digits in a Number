#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

     int count = 0,num;
    scanf("%d", &num);
    if (num== 0) 
     printf("1");
    else{
        while (num != 0){
            num = num/10;
            count++;
        }
        printf("%d", count);
    }
    return 0;
}
