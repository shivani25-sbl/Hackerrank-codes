#include <stdio.h>
#include <ctype.h>  

int main() {
    char S[1001];  
    int uppercase_count = 0;
    int lowercase_count = 0;

   
    fgets(S, sizeof(S), stdin);

    
    for (int i = 0; S[i] != '\0'; i++) {
        if (isupper(S[i])) {
            uppercase_count++;
        } else if (islower(S[i])) {
            lowercase_count++;
        }
    }

    
    printf("%d %d\n", uppercase_count, lowercase_count);

    return 0;
}
