#include <stdio.h>
#include <ctype.h>  

int main() {
    char S[2001];  
    int word_count = 0;
    int in_word = 0;

   
    fgets(S, sizeof(S), stdin);

    
    for (int i = 0; S[i] != '\0'; i++) {
        if (isalpha(S[i])) {
            if (!in_word) {
                word_count++;
                in_word = 1;
            }
        } else {
            in_word = 0;
        }
    }

    
    printf("%d\n", word_count);

    return 0;
}
