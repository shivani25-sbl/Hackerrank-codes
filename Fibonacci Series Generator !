#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

  int main() {
    int i, n, t1 = 0, t2 = 1, nextTerm;
    printf("Enter the number of terms: ");
    scanf("%d", &n);
    printf("Fibonacci Series: ");
    for n=0, n=1, and n=2
    if (n <= 0) {
        printf("Please enter a positive number of terms.\n");
    } else if (n == 1) {
        printf("%d\n", t1);
    } else {
        printf("%d, %d", t1, t2); // Print the first two terms (0 and 1)
        for (i = 3; i <= n; ++i) {
            nextTerm = t1 + t2;
            printf(", %d", nextTerm);
            t1 = t2;
            t2 = nextTerm;
        }
        printf("\n");
    }

    return 0;
}
    return 0;
}
