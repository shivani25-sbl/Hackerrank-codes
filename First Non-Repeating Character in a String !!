#include <stdio.h>
#include <string.h>

int main() {
    char S[1001];
    int freq[256] = {0};  

   
    fgets(S, sizeof(S), stdin);

    
    size_t len = strlen(S);
    if (len > 0 && S[len - 1] == '\n') {
        S[len - 1] = '\0';
        len--;
    }

    
    for (int i = 0; S[i] != '\0'; i++) {
        freq[(unsigned char)S[i]]++;
    }

    
    int found = 0;
    for (int i = 0; S[i] != '\0'; i++) {
        if (freq[(unsigned char)S[i]] == 1) {
            printf("%c\n", S[i]);
            found = 1;
            break;
        }
    }

    if (!found) {
        printf("-1\n");
    }

    return 0;
}

