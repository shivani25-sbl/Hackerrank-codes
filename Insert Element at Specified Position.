#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int arr[101];
    for(int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    int X, P;
    scanf("%d", &X);
    scanf("%d", &P);
    for(int i = N; i > P; i--) {
        arr[i] = arr[i - 1];
    }
    arr[P] = X;
    for(int i = 0; i <= N; i++) {
        printf("%d", arr[i]);
        if(i != N) {
            printf(" ");
        }
    }

    return 0;
}
