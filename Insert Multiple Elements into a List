#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int arr[100];
    for(int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    int K;
    scanf("%d", &K);

    int currentSize = N;
    for(int i = 0; i < K; i++) {
        int X, P;
        scanf("%d %d", &X, &P);
        for(int j = currentSize; j > P; j--) {
            arr[j] = arr[j - 1];
        }

        arr[P] = X;

        currentSize++;
    }

    for(int i = 0; i < currentSize; i++) {
        printf("%d", arr[i]);
        if(i != currentSize - 1) {
            printf(" ");
        }
    }

    return 0;
}
