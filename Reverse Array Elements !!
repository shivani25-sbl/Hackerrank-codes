#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n , i;
    scanf("%d",&n);
    int arr[n];
    for(i=0; i<n ;i++){
        scanf("%d",&arr[i]);
    }
    int rev = 0;
    for(i=0;i<n ; i++){
        rev = rev + arr[i];
        printf("%d " , rev);
        
    }
    
    return 0;
}
