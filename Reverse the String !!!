#include <stdio.h>
#include <string.h>

int main() {
    char S[1001]; 

    
    fgets(S, sizeof(S), stdin);

   
    size_t len = strlen(S);
    if (len > 0 && S[len - 1] == '\n') {
        S[len - 1] = '\0';
        len--;
    }

    for (int i = len - 1; i >= 0; i--) {
        putchar(S[i]);
    }

    printf("\n");
    return 0;
}
