#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     int N,i ;
       scanf("%d" , &N);
        int arr[N];
        for( i=0 ; i<N ; i++){
            scanf("%d" , &arr[i]);
}
   int val;
    scanf("%d", &val);
    int index = -1 ; 
    for(i=0; i<N; i++){
        if(arr[i]==val){
            index = i;
            break;
        }
    }
    if(index == -1)
        printf("Element not found");
    else 
        printf("Element found at position %d" , index+1);
    return 0;
}
