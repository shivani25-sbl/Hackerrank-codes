#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int first,  temp , last , num , count= 0 ;
    scanf("%d", &num);
    last = num%10;
    num = num/10;
    temp = num;
    while(temp !=0){
        temp =temp/10;
        count++ ;
        
    }
    first = num/(int)pow(10 , count - 1);
    num = num%(int)pow( 10 , count - 1);
    num = last*pow(10 , count)+ num*10 + first;
    printf("%d", num);
    return 0;
}
